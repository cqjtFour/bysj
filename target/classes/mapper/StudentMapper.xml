  <select id="selectBigZaHui" resultMap="BaseResultMapWithDisciplineAndSign">
    SELECT * from t_qyb,t_xsb,t_zyb,t_xyb where t_qyb.XH = t_xsb.XH
    and t_xsb.ZYBH=t_zyb.ZYBH and t_zyb.XYBH = t_xyb.XYBH
  </select>

  <select id="queryStudentWithSign" resultMap="BaseResultMapWithDisciplineAndSign">
    SELECT * from t_qyb,t_xsb,t_zyb,t_xyb
    <where>
      t_qyb.XH = t_xsb.XH
      and t_xsb.ZYBH=t_zyb.ZYBH and t_zyb.XYBH = t_xyb.XYBH
      and bysjs = #{year}
      <if test="collega != null and collega !=''">
        AND t_zyb.XYBH = #{collega}
      </if>
      <if test="sure != null ">
        AND sfqdsf = #{sure}
      </if>
    </where>
  </select>


  <sql id="studentAllData">
    *
  </sql>

  <select id="queryStudentByInput" resultMap="BaseResultMap" parameterType="java.lang.String">
    <bind name="pattern" value="'%' + param + '%'" />
    SELECT
    <include refid="studentAllData" />
    from t_xsb where XSXM LIKE #{pattern}
                    or XH like #{pattern}
                    or XL like #{pattern}
                    or bkzy like #{pattern}
                    or sszy LIKE #{pattern}
                    OR BYSJS LIKE #{pattern}
                    or qwhy like #{pattern}
                    or qwgzdd like #{pattern}
                    or GXSJ LIKE #{pattern}
  </select>

  <select id="querySomeStudent" resultMap="BaseResultMap">
    SELECT * from t_xsb
    <where>
    <if test=" stuName != null and stuName != ''">
       and XSXM = #{stuName}
    </if>
    <if test=" stuNo != null and stuNo != ''">
      and XH = #{stuNo}
    </if>
    </where>
  </select>

  <select id="selectStudentWithDis" resultMap="BaseResultMapWithJoin">
   Select xsxm,zymc from t_xsb,t_zyb where t_xsb.zybh=t_zyb.zybh and xh=#{xsxh}
